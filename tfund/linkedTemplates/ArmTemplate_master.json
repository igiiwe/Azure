{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "tfund"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"Northwind_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Northwind'"
		},
		"Northwind_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Northwind'"
		},
		"NorthwindDest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'NorthwindDest'"
		},
		"container_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'container'"
		},
		"LDE22H64Q2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LDE22H64Q2'"
		},
		"Northwind_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "EUROPE\\oasobogungod"
		},
		"OnPremSSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "tfund.database.windows.net"
		},
		"OnPremSSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
			"type": "string",
			"defaultValue": "igiiwe"
		},
		"OnPremSSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
			"type": "secureString"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"NorthwindDest_connectionString": {
						"value": "[parameters('NorthwindDest_connectionString')]"
					},
					"container_connectionString": {
						"value": "[parameters('container_connectionString')]"
					},
					"LDE22H64Q2_connectionString": {
						"value": "[parameters('LDE22H64Q2_connectionString')]"
					},
					"Northwind_connectionString": {
						"value": "[parameters('Northwind_connectionString')]"
					},
					"Northwind_password": {
						"value": "[parameters('Northwind_password')]"
					},
					"OnPremSSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
						"value": "[parameters('OnPremSSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint')]"
					},
					"OnPremSSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
						"value": "[parameters('OnPremSSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName')]"
					},
					"OnPremSSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
						"value": "[parameters('OnPremSSIS_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword')]"
					},
					"Northwind_properties_typeProperties_userName": {
						"value": "[parameters('Northwind_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}